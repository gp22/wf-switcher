{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wazframe/wf-switcher",
	"version": "0.1.0",
	"title": "Switcher",
	"category": "design",
	"icon": "grid-view",
	"description": "Every Layout components.",
	"supports": {
		"html": false
	},
	"textdomain": "wf-switcher",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"threshold": {
			"type": "string",
			"default": "30rem"
		},
		"space": {
			"type": "string",
			"default": "1.5rem"
		},
		"limit": {
			"type": "number",
			"default": 3
		}
	}
}
